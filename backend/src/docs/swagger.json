			"SignupDto": {
				"description": "DTO for user signup\r\n\r\nValidates all required fields for creating a new user account.\r\nThe username, email, and phone number combination must be unique.",
				"properties": {
					"email": {
						"type": "string",
						"description": "User's email address (10-60 characters)",
						"example": "user@example.com"
					},
					"password": {
						"type": "string",
						"description": "Password (6-60 characters, must contain uppercase, lowercase, and number)",
						"example": "SecurePass123"
					},
					"username": {
						"type": "string",
						"description": "Unique username (3-30 characters, alphanumeric and underscores only)",
						"example": "johndoe"
					},
					"firstName": {
						"type": "string",
						"description": "First name (2-50 characters)",
						"example": "John"
					},
					"lastName": {
						"type": "string",
						"description": "Last name (2-50 characters)",
						"example": "Doe"
					},
					"phoneCountryCode": {
						"type": "number",
						"format": "double",
						"description": "Phone country code (1-999)",
						"example": 1
					},
					"phoneNumber": {
						"type": "number",
						"format": "double",
						"description": "Phone number (4-15 digits)",
						"example": 1234567890
					},
					"yearOfBirth": {
						"type": "number",
						"format": "double",
						"description": "Year of birth (must be 13+ years old)",
						"example": 1990
					},
					"location": {
						"type": "string",
						"description": "User's location (1-100 characters)",
						"example": "New York, NY"
					},
					"userType": {
						"type": "string",
						"enum": [
							"musician",
							"service_provider",
							"other"
						],
						"description": "User type: \"musician\", \"service_provider\", or \"other\"",
						"example": "musician"
					}
				},
				"required": [
					"email",
					"password",
					"username",
					"firstName",
					"lastName",
					"phoneCountryCode",
					"phoneNumber",
					"yearOfBirth",
					"location",
					"userType"
				],
				"type": "object",
				"additionalProperties": false,
				"example": {
					"email": "user@example.com",
					"password": "SecurePass123",
					"username": "johndoe",
					"firstName": "John",
					"lastName": "Doe",
