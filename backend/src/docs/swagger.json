			"UpdateProfileDto": {
				"description": "DTO for updating user profile\r\n\r\nUsed to update user profile information. All fields are optional - only\r\nprovided fields will be updated. The user_id is automatically extracted\r\nfrom the authentication token.",
				"properties": {
					"firstName": {
						"type": "string",
						"description": "First name (1-50 characters)",
						"example": "John"
					},
					"lastName": {
						"type": "string",
						"description": "Last name (1-50 characters)",
						"example": "Doe"
					},
					"bio": {
						"type": "string",
						"description": "Short bio (max 100 characters)",
						"example": "Musician from New York"
					},
					"aboutMe": {
						"type": "string",
						"description": "Detailed about me section (max 500 characters)",
						"example": "I'm a professional musician with 10 years of experience..."
					},
					"avatarUrl": {
						"type": "string",
						"description": "URL to user's avatar image",
						"example": "https://example.com/avatar.jpg"
					},
					"location": {
						"type": "string",
						"description": "User's location (1-100 characters)",
						"example": "New York, NY"
					},
					"themeColor": {
						"type": "string",
						"description": "Theme color in hex format",
						"example": "#FF5733"
					},
					"spotifyPlaylistUrl": {
						"type": "string",
						"description": "URL to Spotify playlist",
						"example": "https://open.spotify.com/playlist/..."
					},
					"phoneCountryCode": {
						"type": "number",
						"format": "double",
						"description": "Phone country code (1-999)",
						"example": 1
					},
					"phoneNumber": {
						"type": "number",
						"format": "double",
						"description": "Phone number (4-15 digits)",
						"example": 1234567890
					},
					"onboardingCompleted": {
						"type": "boolean",
						"description": "Whether user has completed onboarding",
						"example": true
					}
				},
				"type": "object",
				"additionalProperties": false,
				"example": {
					"firstName": "John",
					"lastName": "Doe",
					"bio": "Musician from New York",
					"location": "New York, NY",
					"themeColor": "#FF5733"
				}
			},
