			"CreatePostBody": {
				"description": "DTO for creating a new post\r\n\r\nUsed when creating a post (note, request, or story). The author_id is\r\nautomatically extracted from the authentication token.\r\n\r\nFor \"note\" posts, you can include tags.\r\nFor \"request\" posts, you can include genres and paid_opportunity flag.\r\nAll post types can include media attachments and tagged users.",
				"properties": {
					"description": {
						"type": "string",
						"description": "Post description (10-5000 characters)",
						"example": "I'm looking for an experienced drummer to join our band..."
					},
					"expires_at": {
						"type": "string",
						"nullable": true,
						"description": "Optional expiration date/time (ISO 8601 format)",
						"example": "2024-12-31T23:59:59Z"
					},
					"location": {
						"type": "string",
						"nullable": true,
						"description": "Optional location where the post is relevant",
						"example": "New York, NY"
					},
					"paid_opportunity": {
						"type": "boolean",
						"nullable": true,
						"description": "Whether this is a paid opportunity (only for \"request\" type posts)",
						"example": true
					},
					"title": {
						"type": "string",
						"description": "Post title (1-100 characters)",
						"example": "Looking for a drummer"
					},
					"type": {
						"$ref": "#/components/schemas/PostType",
						"description": "Post type: \"note\", \"request\", or \"story\"",
						"example": "note"
					},
					"tags": {
						"items": {
							"type": "string"
						},
						"type": "array",
						"description": "Tags for \"note\" type posts",
						"example": [
							"music",
							"jazz",
							"performance"
						]
					},
					"genres": {
						"items": {
							"type": "string"
						},
						"type": "array",
						"description": "Genres for \"request\" type posts",
						"example": [
							"rock",
							"pop",
							"indie"
						]
					},
					"taggedUsers": {
						"items": {
							"type": "string"
						},
						"type": "array",
						"description": "Array of user IDs to tag in the post",
						"example": [
							"123e4567-e89b-12d3-a456-426614174000"
						]
					},
					"media": {
						"items": {
							"$ref": "#/components/schemas/MediaItemDto"
						},
						"type": "array",
						"description": "Media attachments (images or videos)",
						"example": [
							{
								"url": "https://example.com/image.jpg",
								"type": "image"
							}
						]
					}
				},
				"required": [
					"description",
					"title",
					"type"
				],
				"type": "object",
				"additionalProperties": false,
				"example": {
					"type": "note",
					"title": "My First Post",
					"description": "This is a detailed description of my post",
					"location": "New York, NY",
					"tags": [
						"music",
						"jazz"
					],
					"media": [
						{
							"url": "https://example.com/image.jpg",
							"type": "image"
						}
					]
				}
			},
